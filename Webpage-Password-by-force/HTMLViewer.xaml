<Window x:Class="Webpage_Password_by_force.HTMLViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Webpage_Password_by_force"
        mc:Ignorable="d"
        Title="HTMLViewer" Height="450" Width="800">
    <Grid Background="#FF036891">
        <TextBox x:Name="HTMLTEXT" Margin="10,10,10,5" Text="TextBox" TextWrapping="Wrap" Foreground="#FF03D2FE" Background="#FF036891" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto"/>

    </Grid>
</Window>
